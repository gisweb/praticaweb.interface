<?xml version="1.0"?>
<object name="portal_skins">

  <object name="praticaweb_interface_scripts"
      meta_type="Filesystem Directory View"
      directory="praticaweb.interface:skins/praticaweb_interface_scripts"/>
  <object name="praticaweb_interface_templates"
      meta_type="Filesystem Directory View"
      directory="praticaweb.interface:skins/praticaweb_interface_templates"/>
  <skin-path name="*">
    <layer name="praticaweb_interface_scripts"
        insert-after="custom"/>
    <layer name="praticaweb_interface_templates"
        insert-after="praticaweb_interface_scripts"/>
  </skin-path>

</object>